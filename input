#https://www.acmicpc.net/problem/26274

k=[]
p=[]
node=[]

N,K,Q = input().split()
N =int(N)
K =int(K)
Q =int(Q)

node = [[] for _ in range(N)]
    
for i in range(N-1):
    u,v = input().split()
    node[int(u)-1].append(int(v))
    node[int(v)-1].append(int(u))
    
k = list(map(int, input().split()))

for i in range(Q):
    A,B = input().split()
    A=int(A)
    B=int(B)


